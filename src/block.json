{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "elpuasdigitalcrafts/spacerify",
	"version": "0.1.0",
	"title": "Spacerify",
	"category": "design",
	"icon": "editor-insertmore",
	"description": "Spacerify: A responsive spacer block with customizable values for desktop and mobile layouts.",
	"supports": {
		"html": false,
		"color": {
			"text": false,
			"gradients": true,
			"background": true
		},
		"align": ["wide", "full"]
	},
	"attributes": {
		"desktopHeight": {
			"type": "number",
			"default": "24"
		},
		"mobileHeight": {
			"type": "number",
			"default": "24"
		}
	},
	"textdomain": "spacerify",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
